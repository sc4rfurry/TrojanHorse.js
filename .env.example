# TrojanHorse.js Environment Variables
# Copy this to .env and fill in your API keys

NODE_ENV=development
ALIENVAULT_API_KEY=your_alienVault_api_key_here
ABUSEIPDB_API_KEY=your_abuseipdb_api_key_here
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here
CROWDSEC_API_KEY=your_crowdsec_api_key_here

# Security
JWT_SECRET=generate_a_secure_random_string_here
ENCRYPTION_KEY=generate_a_32_byte_random_key_here

# Server Configuration (for API deployment)
PORT=3000
API_USERNAME=admin
API_PASSWORD_HASH=generate_bcrypt_hash_here

# Monitoring (optional)
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=info
